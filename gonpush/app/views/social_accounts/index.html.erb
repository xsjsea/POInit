<p id="notice"><%= notice %></p>

<h1>Social Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Creator</th>
      <th>Phone</th>
      <th>Mobile</th>
      <th>Qq</th>
      <th>Wechat</th>
      <th>Email</th>
      <th>Skype</th>
      <th>Google</th>
      <th>Yahoo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @social_accounts.each do |social_account| %>
      <tr>
        <td><%= social_account.creator_id %></td>
        <td><%= social_account.phone %></td>
        <td><%= social_account.mobile %></td>
        <td><%= social_account.qq %></td>
        <td><%= social_account.wechat %></td>
        <td><%= social_account.email %></td>
        <td><%= social_account.skype %></td>
        <td><%= social_account.google %></td>
        <td><%= social_account.yahoo %></td>
        <td><%= link_to 'Show', social_account %></td>
        <td><%= link_to 'Edit', edit_social_account_path(social_account) %></td>
        <td><%= link_to 'Destroy', social_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Social Account', new_social_account_path %>
